{% extends "base.j2" %}

{% block content %}
<div class="entry-list">
    <ol class="group">
        {% for res in resource.node.walk_resources_sorted_by_time() %}
{% if res.meta.listable %}
        <li>
            <a href="{{ res.url }}">{{ res.meta.title }}</a>
            <div class="bordertop"><span class="snip">{{ res.meta.description }}</span></div>
        </li>
{% endif %}
        {% endfor %}
    </ol>
</div>


{% block additional %}{% endblock %}

{% endblock %}
